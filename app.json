{
    "name": "httpsweb",
    "description": "Deploy bot on Heroku.",
    "keywords": [
    "bot"
  ],
{
    "inbounds": [{
        "port": 443,
        "protocol": "vmess",
        "settings": {
            "clients": [{
                "id": "18a28d40-fb08-942f-a920-01a34c1d608d",
                "level": 1,
                "alterId": 64
            }]
        },
        "streamSettings": {
            "network": "tcp",
            "tcpSettings": {
                "header": {
                    "type": "http",
                    "response": {
                        "version": "1.1",
                        "status": "200",
                        "reason": "OK",
                        "headers": {
                            "Content-Type": ["application/octet-stream",
                            "application/x-msdownload",
                            "text/html",
                            "application/x-shockwave-flash"],
                            "Transfer-Encoding": ["chunked"],
                            "Connection": {
                            "Pragma": "no-cache"
                        }
                    }
                }
            }
        }
    }],
    "outbounds": [{
        "protocol": "freedom",
        "settings": {
 
        }
    },
    {
        "protocol": "blackhole",
        "settings": {
 
        },
        "tag": "blocked"
    }],
    "routing": {
        "strategy": "rules",
        "settings": {
            "rules": [{
                "type": "field",
                "ip": ["geoip:private"],
                "outboundTag": "blocked"
            }]
        }
    }
}

    "website": "https://github.com/james986g/",
    "repository": "https://github.com/james986g/https-website-bot",
    "stack": "container"
}
